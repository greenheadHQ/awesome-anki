{
  "name": "@anki-splitter/server",
  "version": "1.0.0",
  "type": "module",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "bunx oxlint src && bunx oxfmt --check src",
    "test": "bun test --pass-with-no-tests",
    "start": "bun run src/index.ts",
    "history:sync": "bun run src/history-sync.ts"
  },
  "dependencies": {
    "@anki-splitter/core": "workspace:*",
    "dotenv": "^16.4.0",
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "bun-types": "^1.3.2",
    "typescript": "^5.0.0"
  }
}
